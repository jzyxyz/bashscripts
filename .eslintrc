{
    "extends": "airbnb",
    "rules": {
        "semi": "off",
        "import/extensions": "off",
        "no-var": "off",
        "indent": "off",
        "no-console": "off",
        "no-param-reassign": [
            "error",
            {
                "props": false
            }
        ],
        "no-alert": "off",
        "no-underscore-dangle": [
            "error",
            {
                "allow": [
                    "_id"
                ]
            }
        ]
    }
}